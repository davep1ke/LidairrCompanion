<Application x:Class="LidarrCompanion.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LidarrCompanion"
             StartupUri="/Forms/MainWindow/MainWindow.xaml"
             xmlns:helpers="clr-namespace:LidarrCompanion.Helpers">
    <Application.Resources>
        <helpers:ActionToBrushConverter x:Key="ActionToBackgroundConverter" Mode="Background"/>
        <helpers:ActionToBrushConverter x:Key="ActionToForegroundConverter" Mode="Foreground"/>
    </Application.Resources>
</Application>
